{
  "version": 2,
  "builds": [
    {
      "src": "Frontend/event-platform/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/event-platform",
        "buildCommand": "npm run build -- --configuration production",
        "installCommand": "npm install",
        "nodeVersion": "20.11.1"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 