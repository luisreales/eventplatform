{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Angular (event-platform)",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:4200",
      "webRoot": "${workspaceFolder}/Frontend/event-platform",
      "sourceMaps": true,
      "trace": true,
      "skipFiles": ["<node_internals>/**"],
      "preLaunchTask": "npm: start"
    }
  ]
}