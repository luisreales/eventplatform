{
    "version": 2,
    "builds": [
      {
        "src": "Frontend/event-platform/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist/event-platform",
          "buildCommand": "npm run build -- --configuration production",
          "installCommand": "npm install"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  } 